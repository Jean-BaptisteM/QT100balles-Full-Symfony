<form method="post" action="{{ path('admin_item_delete', {'id': item.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cet item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ item.id) }}">
	<button class="btn btn-purple my-2 col-4">Supprimer</button>
</form>

